eckofcardsapi.com/api/deck/${deck_id}/draw`)\n  .then(res => {\n      if (res.status !== 200) {\n        throw new Error('Unsuccessful request to deckofcardsapi.com')\n      }\n        return res.json()\n      })\n  .then(json => {\n        dispatch({\n          type: DECK_DRAW.FETCH_SUCCESS,\n          remaining: json.remaining,\n          cards: json.cards\n        });\n\n  }\n\n  )\n  .catch(err => dispatch({ type: 'DECK_DRAW.FETCH_ERROR', message: err.message}))\n  ;\n}\n"},"lineCount":null}},"error":null,"hash":"784a6fe92cc8b60c37ac5a6b5ec2afff","cacheData":{"env":{}}}